{
  "name": "nuxtation",
  "type": "module",
  "version": "4.0.0",
  "private": true,
  "packageManager": "pnpm@10.25.0",
  "description": "Nuxtation project",
  "author": "annrie <annrie@users.noreply.github.com>",
  "license": "MIT",
  "main": "./nuxt.config.ts",
  "scripts": {
    "build": "VITE_APP_ENV=production NODE_OPTIONS='--max-old-space-size=16384' nuxi build",
    "dev": "nuxi dev -o",
    "start": "node .output/server/index.mjs",
    "generate": "VITE_APP_ENV=production NODE_OPTIONS='--max-old-space-size=16384' nuxi generate",
    "preview": "nuxi preview",
    "npxprev": "npx serve dist",
    "mdts": "npx mdts",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:md": "markdownlint-cli2 '**/*.md'",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "oco": "opencommit",
    "lloco": "OCO_AI_PROVIDER='ollama' opencommit",
    "lighthouse": "./scripts/lighthouse-production.sh --accurate"
  },
